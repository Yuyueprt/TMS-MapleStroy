// TwMS v228.1 CRC Auto Key
// 優點:不衝突遊戲按鍵
//PgUp   01490000    //1864
//PgDown 01510000    //1894
//Insert 01520000    //184c
//Delete 01530000    //187c
//Home   01470000    //1858
//End    014f0000    //1888
//Shift  00360005
//Ctrl   001D0030

[Enable]
Alloc(Hook,128)
Alloc(PressKey,128)
Label(Return)
Label(Exit)

Hook:
push eax
mov eax,[044FB438]
mov eax,[eax+14]
mov eax,[eax+19218]
cmp eax,1
pop eax
jb Exit
pushad
mov edx,001D0030
call PressKey
popad
call 019A5910
jmp Return

PressKey:
mov esi,[04251EB8]
mov ecx,[esi+A4]
push edx
push 00
call 02D183E0
ret

Exit:
call 019A5910
jmp Return

0159CCE0:
jmp Hook
Return:

[Disable]
0159CCE0:
call 019A5910

DeAlloc(Hook)
DeAlloc(PressKey)
